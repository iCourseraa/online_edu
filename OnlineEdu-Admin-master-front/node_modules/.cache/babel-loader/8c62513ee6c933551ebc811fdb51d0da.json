n        <div style=\"float: left\">课程封面：</div>\r\n        <el-image :src=\"encodeOssFileUri(course.cover)\" fit=\"cover\" style=\"width: 200px;height: 100px\" />\r\n      </el-col>\r\n      <el-col :span=\"12\">课程标题：{{ course.title }}</el-col>\r\n      <el-col :span=\"12\">课程分类：{{ getDetailsSubject(course.subjectParent) }}</el-col>\r\n      <!-- <el-col :span=\"12\">价格分类：{{ course.price }}</el-col> -->\r\n    </el-row>\r\n    <el-divider>学员信息</el-divider>\r\n    <el-row :gutter=\"20\">\r\n      <el-col :span=\"12\">\r\n        <div style=\"float: left\">学员头像：</div>\r\n        <el-avatar :src=\"encodeOssFileUri(member.avatar)\" fit=\"cover\" style=\"width: 100px;height: 100px\" />\r\n      </el-col>\r\n      <el-col :span=\"12\">学员昵称：{{ member.nickname }}</el-col>\r\n      <el-col :span=\"12\">学员手机：{{ member.mobile }}</el-col>\r\n      <el-col :span=\"12\">学员邮箱：{{ member.email }}</el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport { getInfo as memberInfo } from '@/api/member'\r\nimport { getInfo as courseInfo } from '@/api/course'\r\nimport { encodeOssFileUri } from '@/utils'\r\n\r\nexport default {\r\n  name: 'TrderDetails',\r\n  data() {\r\n    return {\r\n      order: {},\r\n      course: {},\r\n      member: {}\r\n    }\r\n  },\r\n  methods: {\r\n    setData(data) {\r\n      this.order = data\r\n      this.getCourse()\r\n      this.getMember()\r\n    },\r\n    encodeOssFileUri(ossUri) {\r\n      return encodeOssFileUri(ossUri)\r\n    },\r\n    getCourse() {\r\n      courseInfo(this.order.courseId).then(resp => {\r\n        this.course = resp.data\r\n      })\r\n    },\r\n    getMember() {\r\n      memberInfo(this.order.memberId).then(resp => {\r\n        this.member = resp.data\r\n      })\r\n    },\r\n    getDetailsSubject(subjectParent) {\r\n      let subject = ''\r\n      let parent = subjectParent\r\n      while (parent) {\r\n        subject = `${parent.title} > ${subject}`\r\n        parent = parent.parent\r\n      }\r\n      return subject.substring(0, subject.length - 3)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.el-col {\r\n  margin-bottom: 20px;\r\n\r\n  &:last-child {\r\n    margin-bottom: 0;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;AA2BA,SAAAA,OAAA,IAAAC,UAAA;AACA,SAAAD,OAAA,IAAAE,UAAA;AACA,SAAAC,gBAAA,IAAAA,iBAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,KAAA;MACAC,MAAA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA,WAAAA,QAAAL,IAAA;MACA,KAAAC,KAAA,GAAAD,IAAA;MACA,KAAAM,SAAA;MACA,KAAAC,SAAA;IACA;IACAT,gBAAA,WAAAA,iBAAAU,MAAA;MACA,OAAAV,iBAAA,CAAAU,MAAA;IACA;IACAF,SAAA,WAAAA,UAAA;MAAA,IAAAG,KAAA;MACAZ,UAAA,MAAAI,KAAA,CAAAS,QAAA,EAAAC,IAAA,WAAAC,IAAA;QACAH,KAAA,CAAAP,MAAA,GAAAU,IAAA,CAAAZ,IAAA;MACA;IACA;IACAO,SAAA,WAAAA,UAAA;MAAA,IAAAM,MAAA;MACAjB,UAAA,MAAAK,KAAA,CAAAa,QAAA,EAAAH,IAAA,WAAAC,IAAA;QACAC,MAAA,CAAAV,MAAA,GAAAS,IAAA,CAAAZ,IAAA;MACA;IACA;IACAe,iBAAA,WAAAA,kBAAAC,aAAA;MACA,IAAAC,OAAA;MACA,IAAAC,MAAA,GAAAF,aAAA;MACA,OAAAE,MAAA;QACAD,OAAA,MAAAE,MAAA,CAAAD,MAAA,CAAAE,KAAA,SAAAD,MAAA,CAAAF,OAAA;QACAC,MAAA,GAAAA,MAAA,CAAAA,MAAA;MACA;MACA,OAAAD,OAAA,CAAAI,SAAA,IAAAJ,OAAA,CAAAK,MAAA;IACA;EACA;AACA","ignoreList":[]}]}