et'\n  })\n}\n\n// 创建课程\nexport function createIt(data) {\n  return request({\n    url: `${api_prefix}/create`,\n    method: 'post',\n    headers: { 'Content-Type': 'multipart/form-data' },\n    data\n  })\n}\n\n// 删除\nexport function deleteIt(id) {\n  return request({\n    url: `${api_prefix}/delete/${id}`,\n    method: 'post'\n  })\n}\n\n// 上传图片\nexport function uploadPic(data) {\n  return request({\n    url: `${api_prefix}/upload/pic`,\n    method: 'post',\n    headers: { 'Content-Type': 'multipart/form-data' },\n    data\n  })\n}\n\n// 修改信息\nexport function updateIt(data) {\n  return request({\n    url: `${api_prefix}/update`,\n    method: 'post',\n    headers: { 'Content-Type': 'multipart/form-data' },\n    data\n  })\n}\n\n// 修改章节与视频信息\nexport function updateCV(courseId) {\n  return request({\n    url: `${api_prefix}/update/${courseId}`,\n    method: 'post'\n  })\n}\n\n"],"mappings":";AAAA,OAAOA,OAAO,MAAM,iBAAiB;AAErC,IAAMC,UAAU,GAAG,qBAAqB;;AAExC;AACA,OAAO,SAASC,QAAQA,CAAA,EAAG;EACzB,OAAOF,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,WAAQ;IAC1BI,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASC,QAAQA,CAACC,EAAE,EAAE;EAC3B,OAAOP,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,cAAAG,MAAA,CAAWG,EAAE,CAAE;IACjCF,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASG,OAAOA,CAACD,EAAE,EAAE;EAC1B,OAAOP,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,YAAAG,MAAA,CAASG,EAAE,CAAE;IAC/BF,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASI,IAAIA,CAACC,IAAI,EAAE;EACzB,OAAOV,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,UAAO;IACzBI,MAAM,EAAE,MAAM;IACdK,IAAI,EAAJA;EACF,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASC,OAAOA,CAAA,EAAG;EACxB,OAAOX,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,cAAW;IAC7BI,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASO,QAAQA,CAACF,IAAI,EAAE;EAC7B,OAAOV,OAAO,CAAC;IACbG,GAAG,KAAAC,MAAA,CAAKH,UAAU,YAAS;IAC3BI,MAAM,EAAE,MAAM;IACdQ,OAAO,EAAE;MAAE,cAAc,EAAE;IAAsB,CAAC;IAClDH,IAAI,EAAJA;EACF,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASI,QAAQA,CAACP,EAAE,EAAE;EAC3B,OAAOP,OAAO,CAAC;IACbG,G