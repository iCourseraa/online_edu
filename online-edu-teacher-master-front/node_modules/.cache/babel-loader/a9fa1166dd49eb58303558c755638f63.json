)/online-edu-teacher-master-front/src/components/verify/index.js"],"sourcesContent":["import Vue from 'vue'\r\nimport Main from './main.vue'\r\nimport { isEmpty } from '@/utils'\r\n\r\nconst Constructor = Vue.extend(Main)\r\n\r\nconst PuzzleVerify = function({ left, top } = {}) {\r\n  const instance = new Constructor()\r\n  const position = getPosition({ height: instance.height + 130, width: instance.width + 20, left, top })\r\n\r\n  instance.positionTop = position.top\r\n  instance.positionLeft = position.left\r\n  instance.$mount()\r\n  document.body.appendChild(instance.$el)\r\n  instance.visible = true\r\n\r\n  return new Promise((resolve, reject) => {\r\n    instance.resolve = resolve\r\n    instance.reject = reject\r\n  })\r\n}\r\n\r\nfunction getPosition({ height, width, left, top } = {}) {\r\n  const windowHeight = window.innerHeight\r\n  const windowWidth = window.innerWidth\r\n\r\n  // 若任一位置信息为空，直接返回居中位置\r\n  if (isEmpty(left, top)) {\r\n    return {\r\n      top: center(windowHeight, height),\r\n      left: center(windowWidth, width)\r\n    }\r\n  }\r\n\r\n  // 判断位置是否超出屏幕外\r\n  if (windowWidth < left + width) left = center(windowWidth, width)\r\n  if (windowHeight < top + height) top = center(windowHeight, height)\r\n\r\n  return { left, top }\r\n}\r\n\r\nfunction center(full, part) {\r\n  return Math.round((full - part) / 2)\r\n}\r\n\r\nexport default PuzzleVerify\r\n"],"mappings":";AAAA,OAAOA,GAAG,MAAM,KAAK;AACrB,OAAOC,IAAI,MAAM,YAAY;AAC7B,SAASC,OAAO,QAAQ,SAAS;AAEjC,IAAMC,WAAW,GAAGH,GAAG,CAACI,MAAM,CAACH,IAAI,CAAC;AAEpC,IAAMI,YAAY,GAAG,SAAfA,YAAYA,CAAA,EAAgC;EAAA,IAAAC,IAAA,GAAAC,SAAA,CAAAC,MAAA,QAAAD,SAAA,QAAAE,SAAA,GAAAF,SAAA,MAAJ,CAAC,CAAC;IAAhBG,IAAI,GAAAJ,IAAA,CAAJI,IAAI;IAAEC,GAAG,GAAAL,IAAA,CAAHK,GAAG;EACvC,IAAMC,QAAQ,GAAG,IAAIT,WAAW,CAAC,CAAC;EAClC,IAAMU,QAAQ,GAAGC,WAAW,CAAC;IAAEC,MAAM,EAAEH,QAAQ,CAACG,MAAM,GAAG,GAAG;IAAEC,KAAK,EAAEJ,QAAQ,CAACI,KAAK,GAAG,EAAE;IAAEN,IAAI,EAAJA,IAAI;IAAEC,GAAG,EAAHA;EAAI,CAAC,CAAC;EAEtGC,QAAQ,CAACK,WAAW,GAAGJ,QAAQ