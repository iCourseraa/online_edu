y=\"currTime\" label=\"播放时长\"></el-table-column>\n            <el-table-column property=\"kjNumber\" label=\"快进次数\"></el-table-column>\n            <el-table-column property=\"ktNumber\" label=\"快退次数\"></el-table-column>\n          </el-table>\n        </el-dialog>\n        <div>统计图</div>\n        <div id=\"myChart\" ref=\"myChart\" style=\"width: 100%;height:480px;\"></div>\n      </div>\n</template>\n\n<script>\nimport { listAll , videoNumber } from '@/api/video'\nimport * as echarts from 'echarts'\n\nexport default {\n  name: 'EduCourseistView',\n  data() {\n    return {\n      container: null,\n      videoList: [],\n      show: false,\n      videoDetailList: [],\n      visitList: null,\n      viewShow:false,\n      option:{\n        tooltip : {\n          trigger: 'axis',\n          axisPointer : {            // 坐标轴指示器，坐标轴触发有效\n            type : 'shadow'        // 默认为直线，可选为：'line' | 'shadow'\n          }\n        },\n        legend: {\n          data:['点击播放','点击暂停','完整播放','拖拽次数','完整率','快进次数','快退次数']\n        },\n        toolbox: {\n          show : true,\n          orient: 'vertical',\n          x: 'right',\n          y: 'center',\n          feature : {\n            mark : {show: true},\n            dataView : {show: true, readOnly: false},\n            magicType : {show: true, type: ['line', 'bar', 'stack', 'tiled']},\n            restore : {show: true},\n            saveAsImage : {show: true}\n          }\n        },\n        calculable : true,\n        xAxis : [\n          {\n            type : 'category',\n            data : []\n          }\n        ],\n        yAxis : [\n          {\n            type : 'value'\n          }\n        ],\n        series : [\n          // {\n          //   name:'昵称',\n          //   type:'bar',\n          //   data:[320, 332, 301, 334, 390, 330, 320]\n          // },\n          {\n            name:'点击播放',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          },\n          {\n            name:'点击暂停',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          },\n          {\n            name:'完整播放',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          },\n          {\n            name:'拖拽次数',\n            type:'bar',\n            barWidth : 5,\n            data:[],\n          },\n          {\n            name:'完整率',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          },\n          {\n            name:'快进次数',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          },\n          {\n            name:'快退次数',\n            type:'bar',\n            barWidth : 5,\n            data:[]\n          }\n        ]\n      }\n    }\n  },\n  mounted(){\n    this.selectVideoView()\n  },\n  created() {\n    this.getList()\n  },\n  methods: {\n    getChart(){\n      // 在步骤2的方法1中 通过 this.$echarts.init方法初始化echarts实例并使用 setOption 方法注册图表配置项和数据\n      let myChart = this.$echarts.init(document.getElementById(\"myChart\"));\n      myChart.setOption(this.option);\n    },\n    getList(){\n      listAll().then(res=>{\n        this.videoList = res.data\n      })\n    },\n    selectVideo(item){\n      let data ={\n        videoId : item.id\n      }\n      videoNumber(data).then(res=>{\n        this.videoDetailList = res.data\n        this.show =true\n        console.log(\"视频观看情况\",res)\n      })\n    },\n    selectVideoView(item){\n      let data ={\n        videoId : null\n      }\n      videoNumber(data).then(res=>{\n        this.videoDetailList = res.data\n        for (let i = 0; i < this.videoDetailList.length; i++) {\n          this.option.xAxis[0].data.push(this.videoDetailList[i].title)\n          this.option.series[0].data.push(this.videoDetailList[i].playNumber)\n          this.option.series[1].data.push(this.videoDetailList[i].pauseNumber)\n          this.option.series[2].data.push(this.videoDetailList[i].endedNumber !=0 ?this.videoDetailList[i].endedNumber :'0')\n          this.option.series[3].data.push(this.videoDetailList[i].seekedNumber)\n          this.option.series[4].data.push(this.videoDetailList[i].completionRateNumber / 100)\n          this.option.series[5].data.push(this.videoDetailList[i].kjNumber)\n          this.option.series[6].data.push(this.videoDetailList[i].ktNumber)\n          this.getChart()\n        }\n      })\n    },\n    // renderBarChart(container) {\n    //   const chart = new Chart({\n    //     container,\n    //     autoFit: true,\n    //     height:350,\n    //   })\n    //   console.log(\"this.visitList,this.visitList,\",this.visitList)\n    //   chart.data({\n    //     value: this.visitList,\n    //     transform: [\n    //       {\n    //         type: 'rename',\n    //         'statusLabel': '状态',\n    //         'date': '时间范围',\n    //         'count': '拜访数量',\n    //       }\n    //     ],\n    //   });\n    //   chart\n    //     .line()\n    //     .transform({type: 'groupX', y: 'sum'})\n    //     .encode('x', '时间范围')\n    //     .encode('y', '拜访数量')\n    //     .encode('color', '状态')\n    //     .axis('x',{title:'',})\n    //     .axis('y', {title: '', })\n    //     .encode('shape', 'smooth')\n    //\n    //   // 指定更新动画的时间\n    //   //   渲染可视化\n    //   chart.render();\n    // },\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkCA,SAAAA,OAAA,EAAAC,WAAA;AACA,YAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,SAAA;MACAC,SAAA;MACAC,IAAA;MACAC,eAAA;MACAC,SAAA;MACAC,QAAA;MACAC,MAAA;QACAC,OAAA;UACAC,OAAA;UACAC,WAAA;YAAA;YACAC,IAAA;UACA;QACA;QACAC,MAAA;UACAZ,IAAA;QACA;QACAa,OAAA;UACAV,IAAA;UACAW,MAAA;UACAC,CAAA;UACAC,CAAA;UACAC,OAAA;YACAC,IAAA;cAAAf,IAAA;YAAA;YACAgB,QAAA;cAAAhB,IAAA;cAAAiB,QAAA;YAAA;YACAC,SAAA;cAAAlB,IAAA;cAAAQ,IAAA;YAAA;YACAW,OAAA;cAAAnB,IAAA;YAAA;YACAoB,WAAA;cAAApB,IAAA;YAAA;UACA;QACA;QACAqB,UAAA;QACAC,KAAA,GACA;UACAd,IAAA;UACAX,IAAA;QACA,EACA;QACA0B,KAAA,GACA;UACAf,IAAA;QACA,EACA;QACAgB,MAAA;QACA;Q